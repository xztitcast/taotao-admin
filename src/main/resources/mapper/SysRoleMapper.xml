<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.taotao.admin.mapper.SysRoleMapper" >
  <resultMap id="BaseResultMap" type="com.taotao.admin.entity.SysRole" >
    <id column="role_id" property="roleId" jdbcType="BIGINT" />
    <result column="role_name" property="roleName" jdbcType="VARCHAR" />
    <result column="remark" property="remark" jdbcType="VARCHAR" />
    <result column="adder" property="adder" jdbcType="BIGINT" />
    <result column="created" property="created" jdbcType="TIMESTAMP" />
  </resultMap>
  <sql id="Base_Column_List" >
    role_id, role_name, remark, adder, created
  </sql>
  
  <select id="selectRoleIdList" resultType="java.lang.Long" parameterType="java.lang.Long">
  	select role_id from sys_role where created = #{adder}
  </select>
</mapper>